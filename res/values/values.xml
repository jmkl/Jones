<?xml version="1.0" encoding="utf-8"?>
<resources>
<!-- color for toggles text -->
    <color name="dcsms_togel_teks">#fff</color>
    <color name="dcsms_togel_teks_shadow">#000</color>
<!--     color for tab text -->
    <color name="dcsms_tab_teks">#fff</color>
    <color name="dcsms_tab_teks_shadow">#000</color>
    
<!--     text color for dateview -->
    <color name="dcsms_dateview_color">#fff</color>    
<!--     text color for ongoing/nonotification/latest event -->
    <color name="dcsms_notif_title_color">#fff</color>
    
<!--     text color for notification content -->
    <color name="dcsms_notif_konten_title">#fff</color>
    <color name="dcsms_notif_konten_desc">#fff</color>
    <color name="dcsms_notif_konten_date">#fff</color>
    
    
<!-- battery text, clock, trafficstate default color -->
    <color name="batt_color">#fff</color> 
    <color name="clock_color">#fff</color>
    <color name="trafficstate_color">#fff</color>
    
    
<!--     text size for each item -->
     <dimen name="textbattery_size">22dip</dimen>
    <dimen name="clock_size">22dip</dimen>
    <dimen name="traffic_state_size">16dip</dimen>

<!--     font for text item.. the font file must be store in assets folder -->
    <string name="textbattery_font">Regular.ttf</string>
    <string name="clock_font">Regular.ttf</string>
    <string name="traffic_state_font">Regular.ttf</string>
    
<!--     to allow change only in statusbar area -->
    
     <item type="bool" name="include_statusbar" >true</item>
     
     
<!--     date in notification -->
     <dimen name="notifclock_fontsize">40dip</dimen>
       <dimen name="notifdate_fontsize">18dip</dimen>
     <string name="notifclock_font">Regular.ttf</string>
      <string name="notifdate_font">Regular.ttf</string>
    
<!--        weather font type -->
	<string name="city_font">Regular.ttf</string>
	<string name="humidity_font">Regular.ttf</string>
	<string name="degree_font">Regular.ttf</string>
	<string name="condition_font">Regular.ttf</string>
	
    <color name="recent_app_tv_color">#00ffffff</color>
    
<!--     for weather background add more if u like...its refer to images in drawable folder -->
        <string-array name="weather_background_arai">
        <item >weather_backgroud_1</item>
        <item >weather_backgroud_2</item>
        <item >weather_backgroud_3</item>
        <item >weather_backgroud_4</item>
    </string-array>
    

</resources>